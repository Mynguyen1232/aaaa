    <!-- This is container for login form -->
    <div class="container-fluid" ng-controller="AppCtrl">
        <div class="row ">
            <div class="col-md-12 home mt-1 pt-3" style="background-color: rgb(232, 232, 232);">
                <ol>
                    <li class="list-inline-item "><a class="text-decoration-none text-dark"
